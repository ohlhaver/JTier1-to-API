<h1>New source</h1>

<% form_for(@source) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.text_field 'id' %>
  </p>

  <p>
    <%= f.text_field 'name' %>
  </p>

  <p>
    <%= f.text_field 'url' %>
  </p>

  <p>
    <%= f.text_field 'subscription_type' %>
  </p>

  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', sources_path %>
